/// @description get_keys()
function get_keys() {
	//Updates key variables. Use this to get input, makes easier to customize.
	k_l = keyboard_check_direct(ord("A"))
	k_r = keyboard_check_direct(ord("D"))
	k_u = keyboard_check_direct(ord("W"))
	k_d = keyboard_check_direct(ord("S"))

	k_a = keyboard_check_direct(vk_space)
	k_rl= keyboard_check_direct(ord("R"))
	k_ia= keyboard_check_direct(ord("E"))



}
